{
  "name": "Recommended Video Logger",
  "version": "1.0",
  "description": "Track what YouTube is recommending",
  "permissions": ["storage", "*127.0.0.1*"],
  "browser_action": {
    "default_popup": "popup.html"
   },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": ["logger.js"]
    }
  ],
  "manifest_version": 2
}